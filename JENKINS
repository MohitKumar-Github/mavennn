node{
	stage('SCM checkout'){
		echo 'hello'
		bat  'rmdir mavennn /S /Q'
		bat "git clone https://github.com/MohitKumar-Github/mavennn.git"
	}
	stage('compile package'){
		//get maven home path
		def mvnHome = tool name: 'maven 3.9', type: 'maven'
		echo 'hello maven'
		bat "${mvnhome}/bin/mvn package"
	}

}
